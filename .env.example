# Dataset Service Environment Configuration
# Cloud-Native Kubernetes Deployment

# Application Configuration
LOG_LEVEL=INFO
POD_NAME=dataset-service
WORKER_POOL_SIZE=10
BATCH_SIZE=64
BATCH_COMMIT_SIZE=10  # Number of batches to accumulate before committing to DB

# RabbitMQ Configuration
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_MAX_RETRIES=5

# PostgreSQL Configuration (Cloud SQL)
DB_HOST=127.0.0.1  # localhost when using Cloud SQL Proxy sidecar
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_NAME=mep_database

