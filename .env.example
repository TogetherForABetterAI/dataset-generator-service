# Dataset Service Environment Configuration
# Cloud-Native Kubernetes Deployment

# Application Configuration
LOG_LEVEL=INFO
POD_NAME=dataset-service
WORKER_POOL_SIZE=10
BATCH_SIZE=64
BATCH_COMMIT_SIZE=10  # Number of batches to accumulate before committing to DB

# RabbitMQ Configuration
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_MAX_RETRIES=5

# PostgreSQL Configuration (Cloud SQL)
DB_HOST=dataset-service-db 
DB_PORT=5432  # Internal PostgreSQL port
DB_USER=dataset_user
DB_PASSWORD=dataset_pass
DB_NAME=dataset_db

